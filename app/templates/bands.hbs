<div class="rr-sidebar">
  <ul class="rr-list">
    {{#each this.model as |band|}}
      <li class="rr-list-item">
        <LinkTo class="rr-band-link" @route='bands.band.songs' @model={{band.slug}}>
          {{band.name}}
          <span class="rr-pointer">
            {{fa-icon "angle-right"}}
          </span>
        </LinkTo>
      </li>
    {{/each}}
  </ul>
</div>
<div class="rr-main-panel">
  {{outlet}}
</div>

